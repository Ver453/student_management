@model Student_Management.Models.FacultyModel

@{
    ViewData["Title"] = "Create";
    Layout = "_AdminLayout";
}

<h1>Create</h1>

<h4>FacultyModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
